<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.jian1w.aidlserver.MainActivity"
    tools:showIn="@layout/activity_main"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hello World!"
        tools:layout_editor_absoluteY="248dp"
        tools:layout_editor_absoluteX="154dp" />

    <Button
        android:id="@+id/button_bind"
        android:layout_width="99dp"
        android:layout_height="53dp"
        android:text="bind"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="243dp" />

    <Button
        android:id="@+id/button_unbind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="unbind"
        tools:layout_editor_absoluteY="248dp"
        app:layout_constraintLeft_toRightOf="@+id/button_bind"
        android:layout_marginLeft="95dp" />

    <Button
        android:id="@+id/button_call"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:text="call"
        tools:layout_editor_absoluteX="161dp"
        android:layout_marginTop="98dp"
        app:layout_constraintTop_toBottomOf="@+id/button_bind" />

</android.support.constraint.ConstraintLayout>
